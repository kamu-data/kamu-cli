  "query": "\n                SELECT flow_id FROM flows\n                WHERE\n                    (cast($1 as TEXT[]) IS NULL OR flow_type = ANY($1))\n                    AND (cast($2 as flow_status_type[]) IS NULL or flow_status = ANY($2))\n                    AND (cast($3 as TEXT[]) IS NULL OR initiator = ANY($3))\n                ORDER BY flow_id DESC\n                LIMIT $4 OFFSET $5\n                ",
